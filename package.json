  },
  "author": "Simon Seyock",
  "license": "MIT",
  "dependencies": {
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.1.1",
    "@types/chai": "^4.2.14",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.0",
    "chai": "^4.3.0",
    "jsdoctypeparser": "^9.0.0",
    "lodash": "^4.17.20",
    "mocha": "^8.2.1",
    "rollup": "^2.39.0",
    "ts-node": "^9.1.1",
    "ts-standard": "^10.0.0",
    "typedoc": "^0.20.24"
  },
  "ts-standard": {
    "ignore": [
      "/submodules/",
      "/build/",
      "/pages/"
    ]
  }
