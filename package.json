{
  "name": "jsdoc-type-pratt-parser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run typecheck && npm run lint && npm run test:spec",
    "test:spec": "mocha -r ts-node/register test/**/*.spec.ts",
